// Update solar irradiance
http PUT https://solarchargecontroller.herokuapp.com/api/component/solar_irradiance value=120

// Get Solar Irradiance
http GET https://solarchargecontroller.herokuapp.com/api/component/solar_irradiance

// Send power value
http POST https://solarchargecontroller.herokuapp.com/api/power value=29

// Get power value
http GET https://solarchargecontroller.herokuapp.com/api/power
